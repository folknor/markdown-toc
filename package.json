{
	"name": "markdown-toc",
	"description": "Generate a markdown TOC (table of contents).",
	"version": "2.0.0",
	"author": "Jon Schlinkert (https://github.com/jonschlinkert)",
	"repository": "folknor/markdown-toc",
	"license": "MIT",
	"type": "module",
	"files": [
		"dist"
	],
	"main": "./dist/index.js",
	"bin": {
		"markdown-toc": "./dist/cli.js"
	},
	"engines": {
		"node": ">=24",
		"pnpm": ">=10.0"
	},
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"build:watch": "tsc -p tsconfig.json --watch",
		"prepare": "pnpm run build",
		"clean": "rm -rf dist",
		"start": "node dist/cli.js --help",
		"lint": "pnpm exec biome check",
		"check": "pnpm exec biome check && pnpm run build",
		"test": "node dist/cli.js --help"
	},
	"dependencies": {
		"concat-stream": "2.0.0",
		"diacritics-map": "^0.1.0",
		"gray-matter": "^4.0.3",
		"marked": "^17.0.1",
		"minimist": "^1.2.8",
		"strip-ansi": "^7.1.2"
	},
	"devDependencies": {
		"typescript": "^5.9.3",
		"@types/node": "^25.0.3"
	}
}
